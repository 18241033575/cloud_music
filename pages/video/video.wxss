/* pages/video/video.wxss */
.nav_list {
  /* display: flex; */
  /* flex-direction: row;  */
  white-space: nowrap;
  width: 100vh;
  font-size: 16px;
}
.nav_cell {
  display: inline-block;
  /* flex-direction: ;  */
  padding: 5px 9px;
}
.active {
  color: #dc2c1f;
  border-bottom: 1px solid #dc2c1f;
}
/* 视频列表 */
.video_list .cell {
  margin-top: 20px;
  padding: 0 5%;
  width: 100%;
  height: 120px;
  box-sizing: border-box;
}

.cell video {
  width: 100%;
  height: 100%;
  border-radius: 5px;
  overflow: hidden;
}