/* pages/ranking/ranking.wxss */
.sheet_cell {
  display: flex;
  padding: 20rpx 30rpx;
}

.sheet_img {
  position: relative;
  margin-right: 40rpx;
  width: 200rpx;
  height: 200rpx;
  min-width: 200rpx;
  overflow: hidden;
}

.sheet_img text{
  position: absolute;
  bottom: 0;
  color: #ffffff;
}

.sheet_img image {
  width: 100%;
  height: 100%;
}

.desc {
  flex-grow: 1;
  padding: 20rpx 0;
  max-height: 200rpx;
  font-size: 14px;
  line-height: 1.4;
  box-sizing: border-box;
  overflow: hidden;
}



.title {
  margin-top: 20rpx;
  font-size: 12px;
  color: #666;
}
